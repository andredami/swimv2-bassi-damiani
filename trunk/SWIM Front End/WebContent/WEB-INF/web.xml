<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SWIM Front End</display-name>
  <servlet>
    <display-name>LoginServlet</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>it.polimi.ingsw2.swim.pages.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <display-name>RegistrationServlet</display-name>
    <servlet-name>RegistrationServlet</servlet-name>
    <servlet-class>it.polimi.ingsw2.swim.pages.RegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegistrationServlet</servlet-name>
    <url-pattern>/RegistrationServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>404</error-code>
    <location>/Error/PageNotFound.jsp</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/Error/InternalServerError.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.Exception</exception-type>
    <location>/Error/InternalServerError.jsp</location>
  </error-page>
  <error-page>
    <exception-type>java.lang.RuntimeException</exception-type>
    <location>/Error/InternalServerError.jsp</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>SelectAbilityRegistrationServlet</display-name>
    <servlet-name>SelectAbilityRegistrationServlet</servlet-name>
    <servlet-class>it.polimi.ingsw2.swim.pages.SelectAbilityRegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SelectAbilityRegistrationServlet</servlet-name>
    <url-pattern>/SelectAbilityRegistrationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ValidationServlet</display-name>
    <servlet-name>ValidationServlet</servlet-name>
    <servlet-class>it.polimi.ingsw2.swim.pages.ValidationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ValidationServlet</servlet-name>
    <url-pattern>/ValidationServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>SearchAbilityServlet</display-name>
    <servlet-name>SearchAbilityServlet</servlet-name>
    <servlet-class>it.polimi.ingsw2.swim.pages.SearchAbilityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SearchAbilityServlet</servlet-name>
    <url-pattern>/SearchAbilityServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>InsertAbilityServlet</display-name>
    <servlet-name>InsertAbilityServlet</servlet-name>
    <servlet-class>it.polimi.ingsw2.swim.pages.InsertAbilityServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InsertAbilityServlet</servlet-name>
    <url-pattern>/InsertAbilityServlet</url-pattern>
  </servlet-mapping>
</web-app>