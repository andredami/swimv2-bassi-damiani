<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:ejb="http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
	version="3.0">
	<display-name>SWIM Business Logic</display-name>
	<enterprise-beans>
		<session>
			<ejb-name>Mailer</ejb-name>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/domain</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>localhost:8080</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/port</env-entry-name>
				<env-entry-type>java.lang.Integer</env-entry-type>
				<env-entry-value>587</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/host</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>smtp.gmail.com</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/from</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>swimv2bassidamiani@gmail.com</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/auth</env-entry-name>
				<env-entry-type>java.lang.Boolean</env-entry-type>
				<env-entry-value>true</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/username</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>swimv2bassidamiani@gmail.com</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/password</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>paolobassiandreadamiani</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/protocol</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>TLS</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>it.polimi.ingsw2.swim.session.Mailer/debug</env-entry-name>
				<env-entry-type>java.lang.Boolean</env-entry-type>
				<env-entry-value>false</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>